<ColinInput>
  <Problem type="MO_NLP0">
     <Domain>
	<RealVars num="3">
	    <Lower value="0.0"/>
	    <Upper value="20.0"/>
	</RealVars>
     </Domain>

    <Objectives num="2"/>
    <Constraints>
        <Nonlinear num="2">
          <Leq value="4.0"/>
        </Nonlinear>
     </Constraints>

     <Driver>
        <Command>../../../../python/bin/python example_fn2</Command>
     </Driver>
  </Problem>

